{
  "name": "remix-template-remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:scss && remix build",
    "dev": "remix dev",
    "start": "remix-serve build",
    "build:scss": "npx sass app/styles/scss/:app/styles/css/",
    "dev:scss": "npx sass --watch app/styles/scss/:app/styles/css/"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "prisma": "^3.14.0",
    "sass": "^1.52.1",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
